grep -E "^proto tcp" /etc/openvpn/1194.conf >/dev/null
if [ $? -eq 0 ]; then
proto="proto tcp"
if [[ "$payload" = "" ]]; then
remote="remote $Host 1194"
else
remote="remote $Host:1194@$payload 1194"
fi
proxy="http-proxy $Host 8080"
remote1="remote scrip_xxx 999 udp"

else
proto="proto udp"
remote="remote $Host 1194"
fi

cat > /home/vps/public_html/ovpn/$Name.ovpn <<-zenonnew
auth-user-pass
client
dev tun
$proto
$remote1
$remote
$proxy
connect-retry 1
connect-timeout 120
resolv-retry infinite
route-method exe
nobind
ping 5
ping-restart 30
persist-key
persist-tun
persist-remote-ip
mute-replay-warnings
verb 3
cipher none
comp-lzo
script-security 3

<key>
$(cat /etc/openvpn/client-key.pem)
</key>
<cert>
$(cat /etc/openvpn/client-cert.pem)
</cert>
<ca>
$(cat /etc/openvpn/ca.pem)
</ca>
zenonnew

clear
clear
cr
echo "        ╭━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━━
        ┣ สร้างไฟล์เรียบร้อย
        ┣ โปรดนำลิ้งด้านล่างไปตั้งค่าเพิ่มไฟล์ในเว็บ      
