#!/bin/bash
#DEV BY ZENON.TK
clear
clear
cr 
echo "       ╭━━━━━━━━━━━━╮"
echo "       ┣ 》1: TRUE         "
echo "       ┣ 》2: DTAC           "
echo "       ┣━━━━━━━━━━━━╯"
read -p "       ┣ Namber : " zenonnew

if [[ "$zenonnew" = "1" ]]; then
grep -E "^proto tcp" /etc/openvpn/1194.conf >/dev/null
if [ $? -eq 0 ]; then
read -p "       ┣ Host/IP    : " -e -i $IP Host
read -p "       ┣ ใส่เพโหลด  : " payload
else
read -p "       ┣ Host/IP    : " -e -i $IP Host
fi
read -p "       ╰ ต้องการบันทึกหรือไม่ y/n  : " Edit
if [[ "$Edit" = "y" || "$Edit" = "Y" ]]; then
grep -E "^proto tcp" /etc/openvpn/1194.conf >/dev/null
if [ $? -eq 0 ]; then
proto="proto tcp"
proxy="http-proxy $Host 8080"
remote="remote $Host:1194@$payload 1194"
remote1="remote zenon 999 udp"

else
proto="proto udp"
remote="remote $Host 1194"
fi

cat > /etc/openvpn/true.ovpn << zenonnew
client
dev tun
$proto
$remote1
$remote
$proxy
connect-retry 1
connect-timeout 120
resolv-retry infinite
route-method exe
nobind
ping 5
ping-restart 30
persist-key
persist-tun
persist-remote-ip
mute-replay-warnings
verb 3
cipher none
comp-lzo
script-security 3

zenonnew
grep -E "^port 1194" /etc/openvpn/1194.conf >/dev/null
if [ $? -eq 0 ]; then
echo
else
sed -i 's/1194/443/g' /etc/openvpn/true.ovpn
fi

clear
clear
cr
echo "      ╭━━━━━━━━━━━━╮
      ┣ บันทึกเรียบร้อย
      ╰━━━━━━━━━━━━╯
   "
exit
elif [[ "$Edit" = "n" || "$Edit" = "N" ]]; then
echo "      ╭━━━━━━━━━━━━╮
      ┣ ยกเลิกการบันทึก
      ╰━━━━━━━━━━━━╯
   "
   exit
fi

elif [[ "$zenonnew" = "2" ]]; then
grep -E "^proto tcp" /etc/openvpn/1194.conf >/dev/null
if [ $? -eq 0 ]; then
read -p "       ┣ Host/IP    : " -e -i $IP Host
read -p "       ┣ ใส่เพโหลด  : " payload
else
read -p "       ┣ Host/IP    : " -e -i $IP Host
fi
read -p "       ╰ ต้องการบันทึกหรือไม่ y/n  : " Edit
if [[ "$Edit" = "y" || "$Edit" = "Y" ]]; then
grep -E "^proto tcp" /etc/openvpn/1194.conf >/dev/null
if [ $? -eq 0 ]; then
proto="proto tcp"
proxy="http-proxy $Host 8080"
remote="remote $Host:1194@$payload 1194"
remote1="remote zenon 999 udp"

else
proto="proto udp"
remote="remote $Host 1194"
fi

cat > /etc/openvpn/dtac.ovpn << zenonnew
client
dev tun
$proto
$remote1
$remote
$proxy
connect-retry 1
connect-timeout 120
resolv-retry infinite
route-method exe
nobind
ping 5
ping-restart 30
persist-key
persist-tun
persist-remote-ip
mute-replay-warnings
verb 3
cipher none
comp-lzo
script-security 3

zenonnew
grep -E "^port 1194" /etc/openvpn/1194.conf >/dev/null
if [ $? -eq 0 ]; then
echo
else
sed -i 's/1194/443/g' /etc/openvpn/dtac.ovpn
fi

clear
clear
cr
echo "      ╭━━━━━━━━━━━━╮
      ┣ บันทึกเรียบร้อย
      ╰━━━━━━━━━━━━╯
   "
exit
elif [[ "$Edit" = "n" || "$Edit" = "N" ]]; then
echo "      ╭━━━━━━━━━━━━╮
      ┣ ยกเลิกการบันทึก
      ╰━━━━━━━━━━━━╯
   "
   exit
fi
if [[ "$zenonnew" = "" ]]; then
echo "      ╭━━━━━━━━━━╮
      ┣ ไม่พบข้อมูล
      ╰━━━━━━━━━━╯
   "
   exit
