if [[ $dataexpnum < $datahoje ]]; then
printf "\033[1;33m" "$C1"
printf '%-41s' $user
printf "\033[0m"
printf "\033[1;33m"
echo "$dataexpn"
kill $(ps -u $user |awk '{print $1}') >/dev/null 2>/dev/null ; userdel $user
